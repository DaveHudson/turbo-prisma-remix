{
  "private": true,
  "name": "blog",
  "version": "0.0.0",
  "description": "",
  "license": "",
  "scripts": {
    "build": "NODE_ENV=production npm run build:css && remix build",
    "build:css": "tailwindcss -i ./tailwind.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\" \"npm run dev:node\"",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./app/tailwind.css --watch",
    "dev:remix": "NODE_ENV=development remix watch",
    "dev:node": "NODE_ENV=development nodemon ./build/index.js",
    "lint": "TIMING=1 eslint ./app --ext js,jsx,ts,tsx",
    "postinstall": "remix setup node",
    "start": "NODE_ENV=production node ./build/index.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.9.1",
    "@remix-run/express": "1.2.1",
    "@remix-run/react": "^1.2.1",
    "@remix-run/serve": "^1.2.1",
    "@tiptap/extension-code-block-lowlight": "^2.0.0-beta.68",
    "@tiptap/extension-dropcursor": "^2.0.0-beta.25",
    "@tiptap/extension-gapcursor": "^2.0.0-beta.34",
    "@tiptap/extension-highlight": "^2.0.0-beta.33",
    "@tiptap/extension-image": "^2.0.0-beta.26",
    "@tiptap/extension-link": "^2.0.0-beta.36",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/extension-typography": "^2.0.0-beta.20",
    "@tiptap/react": "^2.0.0-beta.108",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "marked": "^4.0.12",
    "metronome-sh": "^1.0.2",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
